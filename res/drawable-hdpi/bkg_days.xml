<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- item background -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color = "#d0d0d0"/>
        </shape>
    </item>
    <!-- item foreground -->
    <item 
        android:left="1dp"
        android:right="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#dddddd"/>
        </shape>
    </item>
</layer-list>
